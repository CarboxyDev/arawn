{
  "name": "@repo/packages-utils",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "import": "./dist/array.mjs",
      "require": "./dist/array.js"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.mjs",
      "require": "./dist/async.js"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.mjs",
      "require": "./dist/date.js"
    },
    "./number": {
      "types": "./dist/number.d.ts",
      "import": "./dist/number.mjs",
      "require": "./dist/number.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.mjs",
      "require": "./dist/string.js"
    }
  },
  "scripts": {
    "update-exports": "tsx ../../turbo/update-exports.ts .",
    "dev": "pnpm update-exports && tsup --watch",
    "build": "pnpm update-exports && tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
}
